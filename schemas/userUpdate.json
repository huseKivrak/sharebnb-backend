{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/userUpdate.json",
	"type": "object",
	"properties": {
		"password": {
			"type": "string",
			"minLength": 5
		},
		"firstName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 50
		},
		"lastName": {
			"type": "string",
			"minLength": 1,
			"maxLength": 75
		},
		"email": {
			"type": "string",
			"minLength": 6,
			"maxLength": 75,
			"format": "email"
		}
	},
	"additionalProperties": false,
	"required": []
}
